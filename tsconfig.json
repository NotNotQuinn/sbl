{
	"compilerOptions": {
		"target": "ES2021",
		"module": "CommonJS",
		"moduleResolution": "Node16",
		"resolveJsonModule": true,
		"noEmit": true,
		"importHelpers": true,
		"importsNotUsedAsValues": "remove",
		"newLine": "lf",
		"alwaysStrict": true,
		"strict": true,
		"strictNullChecks": true,
		"strictFunctionTypes": true,
		"noFallthroughCasesInSwitch": true,
		"noImplicitAny": true,
		"noImplicitReturns": true,
		"noImplicitOverride": true
	}
}
// Here so vscode actually loads all of the typescript files, without having them open (and doesnt complain about missing declarations)
